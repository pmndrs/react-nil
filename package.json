{
  "name": "react-nil",
  "version": "0.0.3",
  "description": "react null renderer, a react custom renderer that renders nothing but logical components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "echo no tests yet"
