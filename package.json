    "react": "^16.13.1",
    "react-reconciler": "^0.25.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.10",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "scheduler": "^0.20.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
